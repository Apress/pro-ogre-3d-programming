<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="4" />
	<Project>
		<Option title="MeshUpgrader" />
		<Option pch_mode="0" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="..\..\Common\bin\debug\OgreMeshUpgrader.exe" />
				<Option working_dir="..\..\Common\bin\Debug" />
				<Option object_output="..\obj\Debug" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option includeInTargetAll="1" />
				<Option projectResourceIncludeDirsRelation="1" />
				<Compiler>
					<Add option="-g" />
					<Add option="-D_STLP_DEBUG" />
					<Add option="-D_DEBUG" />
				</Compiler>
				<Linker>
					<Add library="OgreMain_d" />
					<Add library="stlportstlg.5.0" />
				</Linker>
			</Target>
			<Target title="Release">
				<Option output="..\..\Common\bin\Release\OgreMeshUpgrader.exe" />
				<Option working_dir="..\..\Common\bin\Release" />
				<Option object_output="..\obj\Release" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option includeInTargetAll="1" />
				<Option projectResourceIncludeDirsRelation="1" />
				<Compiler>
					<Add option="-O3" />
					<Add option="-DNDEBUG" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add library="OgreMain" />
					<Add library="stlport.5.0" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-mthreads" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-fident" />
			<Add option="-D_STLP_NO_CUSTOM_IO" />
			<Add option="-D_STLP_USE_DYNAMIC_LIB" />
			<Add option="-DWIN32" />
			<Add option="-D_WINDOWS" />
			<Add directory="$(#STLPORT_DIR)\stlport" />
			<Add directory="..\include" />
			<Add directory="..\..\..\OgreMain\include" />
		</Compiler>
		<Linker>
			<Add option="-Wl,--enable-auto-image-base" />
			<Add option="-Wl,--add-stdcall-alias" />
			<Add directory="..\..\Common\bin\$(TARGET_NAME)" />
		</Linker>
		<Unit filename="..\src\main.cpp">
			<Option compilerVar="CPP" />
			<Option target="Debug" />
			<Option target="Release" />
		</Unit>
		<Extensions />
	</Project>
</CodeBlocks_project_file>